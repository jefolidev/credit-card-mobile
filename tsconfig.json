{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true
  },
  "extends": "expo/tsconfig.base",
  "paths": {
    "@components/*": [
      "src/components/*"
    ],
    "@screens/*": [
      "src/screens/*"
    ],
    "@utils/*": [
      "src/utils/*"
    ],
    "@assets/*": [
      "src/assets/*"
    ],
    "@hooks/*": [
      "src/hooks/*"
    ],
    "@app/*": [
      "src/app/*"
    ],
    "@src/*": [
      "src/*"
    ],
  },
}